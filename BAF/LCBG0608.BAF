IF 
	Global("LCA_Reset_0608", "BG0608", 1)
THEN 
	RESPONSE #100
		SetGlobal("LCA_Reset_0608", "BG0608", 0)
		ActionOverride("KENT", JumpToPoint([380.309]))
		ActionOverride("KENT", Face(W))
		ActionOverride("LCNEDERL", JumpToPoint([233.399]))
		ActionOverride("LCNEDERL", Face(NNE))
		Continue()
END



IF 
	Global("LCA_ReturnToBG", "GLOBAL", 1)
	!Global("LCA_CreateNPCS_0608_Kent", "BG0608", 1)
	!Dead("KENT")
THEN 
	RESPONSE #100
		SetGlobal("LCA_CreateNPCS_0608_Kent", "BG0608", 1)
		CreateCreature("KENT", [380.309], W)
		Continue()
END

IF 
	Global("LCA_ReturnToBG", "GLOBAL", 1)
	!Global("LCA_CreateNPCS_0608_Nederlok", "BG0608", 1)
THEN 
	RESPONSE #100
		SetGlobal("LCA_CreateNPCS_0608_Nederlok", "BG0608", 1)
		CreateCreature("LCNEDERL", [233.399], NNE)
		Continue()
END

IF
	Global("LCA_ReturnToBG", "GLOBAL", 1)
THEN
	RESPONSE #100
		NoAction()
END
