IF
	Clicked([ANYONE])
	GlobalLT("XA_PlayerStatueTalk", "GLOBAL", 2)
	IsValidForPartyDialogue("XACORWIN")
	InMyArea(Player1)
	InMyArea("XACORWIN")
THEN
	RESPONSE #100
		SetGlobal("XA_PlayerStatueTalk", "GLOBAL", 1)
		ActionOverride("XACORWIN", StartDialog("XACORWIJ", Player1))
		DisplayString(Myself, @0)
END

IF
	Clicked([ANYONE])
THEN
	RESPONSE #100
		DisplayString(Myself, @0)
END