			RESPONSE #100
				SetInterrupt(FALSE)
				SetGlobal("XA_CorwinLeaveForFamilyTicked", "GLOBAL", 2)
				SetGlobal("XA_CorwinLeaveForFamily", "GLOBAL", 1)
				//DisplayStringHead(Player1, @9)
				SetInterrupt(TRUE)
				Continue()
		END
