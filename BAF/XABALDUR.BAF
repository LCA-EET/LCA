	//{ Leave for Family Timer
	
		//{ Set the Timer
		IF
			Global("XA_ReturnToBG", "GLOBAL", 1)
			GlobalLT("XA_CorwinLeaveForFamilyTimerSet", "GLOBAL", 1)
			Global("XA_CorwinMetFamily", "GLOBAL", 1)
		THEN
