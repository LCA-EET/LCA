			GlobalLT("XA_CorwinLeaveForFamilyTicked", "GLOBAL", 1)
			GlobalLT("XA_CorwinMetFamily", "GLOBAL", 2)
			GlobalLT("XA_DukesDebriefed", "GLOBAL", 1)
			Global("XA_HaltRBGTimers", "GLOBAL", 0)
		THEN
