THEN
	RESPONSE #100
		SetGlobal("XA_ProficiencyTalk", "GLOBAL", 3)
		StartCutSceneMode()
		StartCutScene("XALBPRO1")
END

IF
	Global("XA_ProficiencyTalk", "GLOBAL", 5)
	OR(2)
