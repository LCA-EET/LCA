IF
	OR(3)
		See(NearestEnemyOf(Myself))
		AttackedBy([ANYONE], DEFAULT)
		PickPocketFailed([ANYONE])
THEN
	RESPONSE #100
		ReallyForceSpellRES("OHBTELWE",Myself)
END

IF
	ActionListEmpty()
THEN
	RESPONSE #25
		RandomWalk()
	RESPONSE #25
		NoAction()
	RESPONSE #25
		NoAction()
	RESPONSE #25
		NoAction()
END