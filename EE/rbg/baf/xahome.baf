IF
	GlobalLT("XA_LC_CorwinMetFamily", "GLOBAL", 1)
	GlobalLT("XA_LC_GoHome", "XAHOME", 1)
	OR(2)
		!InMyArea("XACORWIN")
		!InMyArea(Player1)
THEN
	RESPONSE #100
		SetGlobal("XA_LC_GoHome", "XAHOME", 1)
		StartCutSceneMode()
		StartCutScene("XAGOHOME")
END

IF
	GlobalLT("XA_LC_CorwinMetFamily", "GLOBAL", 1)
	InMyArea("XACORWIN")
	InMyArea(Player1)
	InMyArea("XAROHMA")
	InMyArea("XAAUDAM")
	GlobalLT("XA_LC_XACS14Started", "GLOBAL", 1)
THEN
