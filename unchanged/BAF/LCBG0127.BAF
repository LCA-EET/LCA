IF 
	Global("LCA_ReturnToBG", "GLOBAL", 1)
	OR(3)
		Exists("Irlentre")
		Exists("DOPZORL")
		Exists("Aldeth")
THEN
	RESPONSE #100
		ActionOverride("Irlentre", DestroySelf())
		ActionOverride("DOPZORL", DestroySelf())
		ActionOverride("Aldeth", DestroySelf())
		Continue()
END

IF
	Global("LCA_ReturnToBG", "GLOBAL", 1)
THEN
	RESPONSE #100
		NoAction()
END
