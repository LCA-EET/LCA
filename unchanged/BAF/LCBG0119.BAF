IF
	Global("LCA_ReturnToBG", "GLOBAL", 1)
	Global("LCA_LantanaraProphecy", "GLOBAL", 1)
THEN
	RESPONSE #100
		SetGlobal("LCA_LantanaraProphecy", "GLOBAL", 2)
		CreateCreature("LCLANTAN", [466.630], NE)
		Continue()
END

IF 
	Global("LCA_ReturnToBG", "GLOBAL", 1)
	InMyArea("Delthyr")
THEN
	RESPONSE #100
		ActionOverride("Delthyr", DestroySelf())
		Continue()
END